    "@angular/compiler-cli": "^21.0.0-next.6",
    "@babel/core": "^7.25.0",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-typescript": "^7.24.7",
    "@types/express": "^5.0.3",
    "@types/jasmine": "^5.1.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.6.2",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-editor-support": "*",

{
  "name": "queueboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --maxWorkers=50% --detectOpenHandles --logHeapUsage --verbose",
    "test:coverage": "jest --coverage",
    "serve:ssr:queueboard": "node dist/queueboard/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.0-next.5",
    "@angular/cdk": "^21.0.0-next.5",
    "@angular/common": "^21.0.0-next.5",
    "@angular/compiler": "^21.0.0-next.5",
    "@angular/core": "^21.0.0-next.5",
    "@angular/forms": "^21.0.0-next.5",
    "@angular/material": "^21.0.0-next.5",
    "@angular/platform-browser": "^21.0.0-next.5",
    "@angular/platform-browser-dynamic": "^21.0.0-next.5",
    "@angular/platform-server": "^21.0.0-next.5",
    "@angular/router": "^21.0.0-next.5",
    "@angular/ssr": "^21.0.0-next.5",
    "@angular/youtube-player": "^21.0.0-next.6",
    "bootstrap": "^5.3.8",
    "express": "^5.1.0",
